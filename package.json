{
  "name": "mongo-migration",
  "version": "1.1.0",
  "description": "MongoDB migration demo",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./test/*",
    "migrate:init": "migrate init --store='./lib/store.js'",
    "migrate:list": "migrate list --store='./lib/store.js'",
    "migrate:create": "./scripts/generator.sh",
    "migrate:create:test": "migrate create --migrations-dir ./test --template-file ./template/test.js",
    "migrate:up": "migrate up --store='./lib/store.js'",
    "migrate:down": "migrate down --store='./lib/store.js'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://bitbucket.org/apac_eco_system/mongo-migration"
  },
  "keywords": [
    "migrate",
    "migration",
    "mongodb",
    "mongoose"
  ],
  "author": "Kevin.W",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "eslint": "^7.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.4",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1"
  },
  "dependencies": {
    "faker": "^4.1.0",
    "migrate": "^1.6.2",
    "mongoose": "^5.9.14"
  }
}
