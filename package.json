{
  "name": "mongo-migration",
  "version": "1.0.0",
  "description": "MongoDB migration demo",
  "main": "index.js",
  "scripts": {
    "test": "mocha ./test/*",
    "migrate:init": "migrate init --store='./lib/store.js'",
    "migrate:list": "migrate list --store='./lib/store.js'",
    "migrate:create": "migrate create --template-file ./template/migration.js",
    "migrate:create:test": "migrate create --migrations-dir ./test --template-file ./template/test.js",
    "migrate:up": "migrate up --store='./lib/store.js'",
    "migrate:down": "migrate down --store='./lib/store.js'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/airyuu/mongo-migration.git"
  },
  "keywords": [
    "migrate",
    "migration",
    "mongodb",
    "mongoose"
  ],
  "author": "Kevin.W",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/airyuu/mongo-migration/issues"
  },
  "homepage": "https://github.com/airyuu/mongo-migration#readme",
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^7.1.2",
    "nyc": "^15.0.1"
  },
  "dependencies": {
    "faker": "^4.1.0",
    "migrate": "^1.6.2",
    "mongoose": "^5.9.14"
  }
}
